keywords = {Electromyography (EMG) signal,Feature extraction,Linear discriminant analysis,Man-machine interface,Multifunction myoelectric control,Pattern recognition,Prosthesis},
number = {8},
pages = {7420--7431},
publisher = {Elsevier Ltd},
title = {{Feature reduction and selection for EMG signal classification}},
url = {http://dx.doi.org/10.1016/j.eswa.2012.01.102},
volume = {39},
