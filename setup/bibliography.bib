file = {:Users/oliver-d/Library/Application Support/Mendeley Desktop/Downloaded/Hahne et al. - 2014 - Linear and Nonlinear Regression Techniques for Simultaneous and Proportional Myoelectric Control.pdf:pdf},
isbn = {1534-4320},
issn = {1534-4320},
journal = {IEEE Transactions on Neural Systems and Rehabilitation Engineering},
number = {2},
pages = {269--279},
pmid = {24608685},
title = {{Linear and Nonlinear Regression Techniques for Simultaneous and Proportional Myoelectric Control}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=6742730},
