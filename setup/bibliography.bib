pmid = {12739757},
title = {{Control of Multifunctional Prosthetic Hands by Processing the Electromyographic Signal}},
url = {http://www.dl.begellhouse.com/journals/4b27cbfc562e21b8,5b51f41866ab7f77,5d2cbc051cfa16a4.html},
